package study02;

public class SwitchCase {

	public static void main(String[] args) {

		int month = 1; //10월
		int day;
		
		
		if(month == 10) {
			day = 31;
			System.out.println(day);
		}else {
			day=0;
			System.out.println("존재하지 않는 달입니다.");
		}
		
//		switch(month) {
//			case 10:
//				day = 31;
//				break;
//			default:
//				day = 0;
//				System.out.println("존재하지 않는 달입니다.");
//		}
//		
//		System.out.println(day);
		
	}

}

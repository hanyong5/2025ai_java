package study02;

public class WhileTest {

	//1~10 합 : 55
	public static void main(String[] args) {
		
		int num = 1;
		int sum = 0;
		
		while( num <= 10 ) {
//			sum = sum + num;
//			num = num + 1;
			
			sum += num;
			num++;
		}
		
		System.out.println("합은 " + sum );
		System.out.println("num " + num);
		
		
		int tsum = 0;
		int i = 1;
		
		while(true) {
			tsum += i;
			i++;
			
			if(i>11) {
				break;
			}
		}
		
		System.out.println(tsum);

	}

}

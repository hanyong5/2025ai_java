package study03;

public class Hero {
	String name;
	String power;
	int attack;
	int defense;
	String rank;
	
	public Hero(String name, String power, int attack, int defense) {
		this.name = name;
		this.power = power;
		this.attack = attack;
		this.defense = defense;
	}

	@Override
	public String toString() {
		return "Hero [name=" + name + ", power=" + power + ", attack=" + attack + ", defense=" + defense + "]";
	}
	
	


	
}
